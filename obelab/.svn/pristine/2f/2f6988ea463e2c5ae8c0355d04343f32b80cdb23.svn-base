.radio-circle input {
	position: absolute !important;
    clip: rect(0, 0, 0, 0);
    height: 0px;
    width: 0px;
    border: 0;
    overflow: hidden;
}
.radio-circle label {
	appearance: none;
	margin:0 7px;
	width: 20px;
	height: 20px;
	background: #ffffff;
	box-shadow: inset 0 0 0 0.4em white, 0 0 0 0.2em;
	border-radius: 50%;
	transition: 0.2s;
	cursor: pointer;
	color: #009edb;
}
.radio-circle label:focus {
	outline: 0;
}
.radio-circle label:hover {
	cursor: pointer;
	background: #009edb;
    box-shadow: inset 0 0 0 0.6em white, 0 0 0 0.3em;
}

.radio-circle input:checked {
	cursor: pointer;
	background: #009edb;
    box-shadow: inset 0 0 0 0.4em white, 0 0 0 0.3em #009edb;
}

.radio-circle input:checked + label {
  background-color: #009edb;
  -webkit-box-shadow: inset 0 0 0 0.4em white, 0 0 0 0.3em #009edb;
  box-shadow: inset 0 0 0 0.4em white, 0 0 0 0.3em #009edb;
  color: #ffffff;
}

.textarea-subject {
    width: 450px;
    height: 200px;
    margin: 0;
    color: #000000;
    border: 2px solid #DFDCDC;
    font-size: 15px;
    resize: none;
    overflow-y: scroll;
}