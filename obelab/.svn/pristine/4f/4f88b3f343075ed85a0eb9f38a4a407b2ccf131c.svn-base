
-- Push List Table

CREATE TABLE `o_push_list` (
  `o_push_num` int(11) NOT NULL AUTO_INCREMENT,
  `o_push_time` datetime NOT NULL,
  `o_push_content` text NOT NULL,
  `o_push_del_status` enum('o','x') NOT NULL DEFAULT 'x',
  `o_push_ref_doc_num` int(11) NOT NULL,
  `o_push_ref_task_num` int(11) NOT NULL,
  PRIMARY KEY (`o_push_num`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
