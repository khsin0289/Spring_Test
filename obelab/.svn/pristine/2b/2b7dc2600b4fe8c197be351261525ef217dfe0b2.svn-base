package dev.web.obelab.bean;

import java.util.Date;

import org.apache.ibatis.type.Alias;

@Alias("device")
public class DeviceBean {
	
	private Integer dvc_num;
	private String dvc_serial_no;
	private String dvc_validate;
	private String dvc_alias;
	private Date dvc_date;
	private String dvc_account_status;
	private Integer dvc_ref_ad_num;
	private Integer dvc_ref_doc_num;
	
	public DeviceBean() {
		// TODO Auto-generated constructor stub
	}

	public Integer getDvc_num() {
		return dvc_num;
	}

	public void setDvc_num(Integer dvc_num) {
		this.dvc_num = dvc_num;
	}

	public String getDvc_serial_no() {
		return dvc_serial_no;
	}

	public void setDvc_serial_no(String dvc_serial_no) {
		this.dvc_serial_no = dvc_serial_no;
	}

	public String getDvc_validate() {
		return dvc_validate;
	}

	public void setDvc_validate(String dvc_validate) {
		this.dvc_validate = dvc_validate;
	}

	public String getDvc_alias() {
		return dvc_alias;
	}

	public void setDvc_alias(String dvc_alias) {
		this.dvc_alias = dvc_alias;
	}

	public Date getDvc_date() {
		return dvc_date;
	}

	public void setDvc_date(Date dvc_date) {
		this.dvc_date = dvc_date;
	}

	public String getDvc_account_status() {
		return dvc_account_status;
	}

	public void setDvc_account_status(String dvc_account_status) {
		this.dvc_account_status = dvc_account_status;
	}

	public Integer getDvc_ref_ad_num() {
		return dvc_ref_ad_num;
	}

	public void setDvc_ref_ad_num(Integer dvc_ref_ad_num) {
		this.dvc_ref_ad_num = dvc_ref_ad_num;
	}

	public Integer getDvc_ref_doc_num() {
		return dvc_ref_doc_num;
	}

	public void setDvc_ref_doc_num(Integer dvc_ref_doc_num) {
		this.dvc_ref_doc_num = dvc_ref_doc_num;
	}
	
}
